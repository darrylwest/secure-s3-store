{
  "name": "secure-s3-store",
  "version": "1.0.1",
  "description": "a secure way to store s3 files",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "clean": "rm -rf dist logs/*.log logs/*.json",
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "test": "jest",
    "test:cov": "jest --coverage",
    "start": "ts-node src/index.ts",
    "e2e": "dotenvx run -- node --loader ts-node/esm e2e/e2e-test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darrylwest/secure-s3-store.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/darrylwest/secure-s3-store/issues"
  },
  "homepage": "https://github.com/darrylwest/secure-s3-store#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.850.0",
    "@dotenvx/dotenvx": "^1.48.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yargs": "^18.0.0",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/node": "^24.1.0",
    "@types/yargs": "^17.0.33",
    "aws-sdk-client-mock": "^4.1.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^30.0.5",
    "jest-mock-extended": "^4.0.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  }
